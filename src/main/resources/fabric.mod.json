{
  "schemaVersion": 1,
  "id": "autoscroll",
  "version": "${version}",
  "name": "Auto Scroll",
  "description": "Automatically scroll through your hotbar quickly for easy block palette randomization.\n${library}",
  "authors": [
    "Roundaround"
  ],
  "contact": {
    "homepage": "https://modrinth.com/mod/auto-scroll",
    "sources": "https://github.com/Roundaround/mc-auto-scroll",
    "issues": "https://github.com/Roundaround/mc-auto-scroll/issues"
  },
  "license": "MIT",
  "icon": "assets/autoscroll/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "me.roundaround.autoscroll.client.AutoScrollClientMod"
    ]
  },
  "mixins": [
    "autoscroll.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.13.3",
    "minecraft": "1.20.*",
    "java": ">=17"
  }
}
